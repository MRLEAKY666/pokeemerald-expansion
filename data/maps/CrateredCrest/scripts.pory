		random(3)
		switch (var(VAR_RESULT)){
		case 0: 
			goto(CrateredCrest_EventScript_OvrWrldMonPosA)
		case 1: 
			goto(CrateredCrest_EventScript_OvrWrldMonPosB)
		case 2: 
			goto(CrateredCrest_EventScript_OvrWrldMonPosC)
		}
	}
	else{
		goto(CrateredCrest_EventScript_OvrWrldMonPosA)
	}
}

