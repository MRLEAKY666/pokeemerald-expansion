	setvar VAR_CRATERED_CREST_RAYQUAZA_SCENE_STATE, 1
	setvar VAR_TEMP_1, 0
	release
	end

CrateredCrest_EventScript_DoMeteoriteSparkle::
	lock
	playse SE_PIN
	dofieldeffectsparkle 52, 11, 0
