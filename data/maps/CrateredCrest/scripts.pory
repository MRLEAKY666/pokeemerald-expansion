CrateredCrest_EventScript_SetupPlayerCloneGfxId::
	checkplayergender
	goto_if_eq VAR_RESULT, MALE, CrateredCrest_EventScript_PlayerCloneMale
	goto_if_eq VAR_RESULT, FEMALE, CrateredCrest_EventScript_PlayerCloneFemale
